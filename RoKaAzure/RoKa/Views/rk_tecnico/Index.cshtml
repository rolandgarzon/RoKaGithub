@model IEnumerable<RoKa.Models.rk_tecnico>

@{
    ViewBag.Title = "Index";
}

<h2>Tecnicos</h2>

<p>
    @Html.ActionLink("Nuevo Tecnico", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.rk_departamento.descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.rk_municipio.descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.rk_pais.descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.identificacion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.telefono)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.activo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.rk_departamento.descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.rk_municipio.descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.rk_pais.descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.identificacion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.telefono)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.activo)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idtecnico }) |
            @Html.ActionLink("Details", "Details", new { id=item.idtecnico }) |
            @*@Html.ActionLink("Delete", "Delete", new { id = item.idtecnico })*@
        </td>
    </tr>
}

</table>
